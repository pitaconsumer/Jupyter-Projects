    "    text = re.sub(r'https?:\\/\\/\\S+','',text) #Remove the hyperlink\n",
    "    \n",
    "    return text\n",
    "\n",
    "#Cleaning the text\n",
    "df['Tweets'] = df['Tweets'].apply(clean_text)\n",
    "df"
